{
  "name": "@freelensapp/kamaji-extension",
  "version": "0.1.0",
  "description": "Kamaji of a Freelens extension",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/freelensapp/freelens-kamaji-extension.git",
    "directory": "."
  },
  "main": "out/main/index.js",
  "renderer": "out/renderer/index.js",
  "files": [
    "out/**/*"
  ],
  "engines": {
    "node": ">= 22.15.1",
    "freelens": "^1.3.2"
  },
  "copyright": "Â© 2025 Freelens Authors",
  "license": "MIT",
  "author": {
    "name": "Freelens Authors",
    "email": "freelens@freelens.app"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "biome": "corepack pnpm dlx @biomejs/biome@1.9.4",
    "biome:check": "corepack pnpm biome check",
    "biome:fix": "corepack pnpm biome check --fix",
    "prebuild": "corepack pnpm typecheck",
    "build": "electron-vite build",
    "bump-version": "corepack pnpm version --no-commit-hooks --no-git-tag-version",
    "clean": "corepack pnpm dlx rimraf@6.0.1 binaries dist static/build",
    "clean:node_modules": "corepack pnpm dlx rimraf@6.0.1 node_modules",
    "knip": "corepack pnpm dlx knip@5.61.0 --dependencies",
    "knip:development": "corepack pnpm knip",
    "knip:production": "corepack pnpm knip --production --strict",
    "lint": "corepack pnpm biome:check",
    "lint:fix": "corepack pnpm biome:fix",
    "typecheck": "tsc --noEmit -p tsconfig.json --composite false"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.27.1",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@freelensapp/extensions": "^1.5.2",
    "@types/node": "^22.15.31",
    "@types/react": "^17.0.87",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^4.5.2",
    "electron-vite": "^3.1.0",
    "mobx": "^6.13.7",
    "mobx-react": "^9.2.0",
    "react": "^17.0.2",
    "sass": "^1.89.2",
    "typescript": "5.8.3",
    "vite": "^6.3.5",
    "vite-plugin-external": "^6.2.2"
  },
  "keywords": [
    "kamaji",
    "control-plane",
    "tenant",
    "extension",
    "freelensapp",
    "lens",
    "openlens",
    "freelens"
  ],
  "packageManager": "pnpm@10.12.4"
}
