{
  "name": "@freelensapp/kamaji-extension",
  "version": "0.1.0",
  "description": "Kamaji of a Freelens extension",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/freelensapp/freelens-kamaji-extension.git",
    "directory": "."
  },
  "main": "out/main/index.js",
  "renderer": "out/renderer/index.js",
  "files": [
    "out/**/*"
  ],
  "engines": {
    "node": ">= 22.15.1",
    "freelens": "^1.3.2"
  },
  "copyright": "Â© 2025 Freelens Authors",
  "license": "MIT",
  "author": {
    "name": "Freelens Authors",
    "email": "freelens@freelens.app"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "biome": "corepack pnpm dlx @biomejs/biome@2.3.8",
    "biome:check": "corepack pnpm biome check",
    "biome:fix": "corepack pnpm biome check --fix",
    "prebuild": "corepack pnpm typecheck",
    "build": "electron-vite build",
    "bump-version": "corepack pnpm version --no-commit-hooks --no-git-tag-version",
    "clean": "corepack pnpm dlx rimraf@6.1.2 binaries dist static/build",
    "clean:node_modules": "corepack pnpm dlx rimraf@6.1.2 node_modules",
    "knip": "corepack pnpm dlx knip@5.71.0 --dependencies --no-gitignore",
    "knip:development": "corepack pnpm knip",
    "knip:production": "corepack pnpm knip --production --strict",
    "lint": "corepack pnpm biome:check",
    "lint:fix": "corepack pnpm biome:fix",
    "typecheck": "tsc --noEmit -p tsconfig.json --composite false"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.28.0",
    "@electron-toolkit/tsconfig": "^2.0.0",
    "@freelensapp/extensions": "^1.3.2",
    "@types/node": "^22.19.1",
    "@types/react": "^17.0.90",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^5.1.2",
    "electron-vite": "^4.0.1",
    "mobx": "^6.15.0",
    "react": "^17.0.2",
    "sass": "^1.94.2",
    "typescript": "5.9.3",
    "vite": "^7.2.6",
    "vite-plugin-external": "^6.2.2",
    "vite-plugin-sass-dts": "^1.3.35"
  },
  "keywords": [
    "kamaji",
    "control-plane",
    "tenant",
    "extension",
    "freelensapp",
    "lens",
    "openlens",
    "freelens"
  ],
  "packageManager": "pnpm@10.24.0"
}
